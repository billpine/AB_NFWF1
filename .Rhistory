sumstats = function(x){
y=x
bstrap <- c()
for (i in 1:1000){
bstrap <- c(bstrap, mean(sample(y,(length(y)),replace=T), na.rm = T))}
c(
Mean=mean(y),
Median=median(y),
SD=sd(y),
Var=var(y),
CV=sd(y)/mean(y),
SE=sd(y)/sqrt(length(y)),
L95SE=mean(y)-1.96*(sd(y)/sqrt(length(y))),
U95SE=mean(y)+1.96*(sd(y)/sqrt(length(y))),
BSMEAN = mean(bstrap),
L95BS = quantile(bstrap,.025),
U95BS= quantile(bstrap,.975))
}
